<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stone.animatedemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InterpolatorActivity"
            android:label="@string/title_activity_interpolator" >
        </activity>
        <activity
            android:name=".listenerTestActivity"
            android:label="@string/title_activity_listener_test" >
        </activity>
        <activity
            android:name=".AnimationSetActivity"
            android:label="@string/title_activity_animation_set" >
        </activity>
    </application>

</manifest>
